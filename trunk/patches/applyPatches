#!/bin/bash
#Patches have been generated by git diff in the top level directory of a git project
OPTS="-p1 -r - --no-backup-if-mismatch --forward"
patch -d build $OPTS < ../patches/build-more-apps.patch
patch -d packages/apps/Settings $OPTS < ../patches/Settings.apk-Vibration-Feedback-Switch-Issue3.patch
patch -d packages/apps/GoogleSearch $OPTS < ../patches/GoogleSearch-conn-manager.patch

